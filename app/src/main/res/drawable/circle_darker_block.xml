<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="oval">
        <size
            android:height="@dimen/circle_view_size"
            android:width="@dimen/circle_view_size"/>
        <stroke
            android:width="@dimen/circle_view_stroke_width_left_right"
            android:dashGap="@dimen/circle_view_stroke_dashGap"
            android:dashWidth="@dimen/circle_view_stroke_dashWidth"
            android:color="@color/red_block_color" />
    </shape>
</item>
</layer-list>